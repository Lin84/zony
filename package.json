{
    "name": "web-star",
    "version": "1.0.0",
    "description": "create the development start kit with webpack",
    "main": "main.js",
    "scripts": {
        "dev": "webpack-dev-server --config ./webpack/webpack.config.js",
        "build": "npm run clean && NODE_ENV=production webpack --config ./webpack/webpack.config.js -p",
        "clean": "rimraf ./dist/*",
        "test": "jest",
        "test-c": "jest --coverage",
        "test-w": "jest --watch",
        "test-w-c": "jest --watch --coverage",
        "json:server": "json-server --watch ./src/api/db.js",
        "dev:server": "concurrently --kill-others \"npm run dev\" \"npm run json:server\""
    },
    "keywords": [
        "webpack",
        "jest",
        "json-server"
    ],
    "author": "Linh Le Tuan",
    "license": "ISC",
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-latest": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "bootstrap-loader": "^2.2.0",
        "bootstrap-sass": "^3.3.7",
        "browser-sync": "^2.18.13",
        "browser-sync-webpack-plugin": "^1.2.0",
        "concurrently": "^3.5.1",
        "css-loader": "^0.28.5",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.3",
        "enzyme-to-json": "^3.1.4",
        "eslint": "^4.18.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-flowtype": "^2.44.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-node": "^6.0.0",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-react": "^7.6.1",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-loader": "^0.5.1",
